{
  "private": true,
  "name": "CheatSheets",
  "description": "",
  "license": "",
  "prisma": {
    "preseed": "prisma db push --force-reset",
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./app/styles/index.css -o ./app/index.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./app/styles/index.css -o ./app/index.css --watch --config ./tailwind.config.cjs",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.3",
    "@prisma/client": "^3.8.0",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@tailwindcss/forms": "^0.4.0",
    "argon2": "^0.28.3",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "docx": "^7.3.0",
    "form-data-to-object": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0",
    "ts-pattern": "^3.3.5",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/faker": "^6.6.9",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "cypress": "^9.4.1",
    "esbuild-register": "^3.3.2",
    "prisma": "^3.8.0",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
